    "class TrainingVisualizer:\n",
    "    def __init__(self):\n",
    "        self.scores = []\n",
    "        self.d_losses = []\n",
    "        self.g_losses = []\n",
    "        self.epsilon_values = []\n",
    "        \n",
    "    def setup_plots(self):\n",
    "        plt.figure(figsize=(20, 10))\n",
    "        plt.clf()\n",
    "        \n",
    "    def update_metrics(self, score, d_loss, g_loss, epsilon):\n",
    "        self.scores.append(score)\n",
    "        self.d_losses.append(d_loss)\n",
    "        self.g_losses.append(g_loss)\n",
    "        self.epsilon_values.append(epsilon)\n",
    "        \n",
    "    def plot_metrics(self, episode):\n",
    "        self.setup_plots()\n",
    "        \n",
    "        # Plot game scores\n",
    "        plt.subplot(2, 2, 1)\n",
    "        plt.plot(self.scores, 'b-', label='Score')\n",
    "        plt.title('Game Scores')\n",
    "        plt.xlabel('Episode')\n",
    "        plt.ylabel('Score')\n",
    "        \n",
    "        # Plot GAN losses\n",
    "        plt.subplot(2, 2, 2)\n",
    "        plt.plot(self.d_losses, 'r-', label='Discriminator Loss')\n",
    "        plt.plot(self.g_losses, 'g-', label='Generator Loss')\n",
    "        plt.title('GAN Training Losses')\n",
    "        plt.xlabel('Episode')\n",
    "        plt.ylabel('Loss')\n",
    "        plt.legend()\n",
    "        \n",
    "        # Plot epsilon value\n",
    "        plt.subplot(2, 2, 3)\n",
    "        plt.plot(self.epsilon_values, 'y-', label='Epsilon')\n",
    "        plt.title('Exploration Rate (Epsilon)')\n",
    "        plt.xlabel('Episode')\n",
    "        plt.ylabel('Epsilon')\n",
    "        \n",
    "        # Plot latest game state\n",
    "        if len(self.scores) > 0:\n",
    "            plt.subplot(2, 2, 4)\n",
    "            plt.imshow(self.latest_state, cmap='gray')\n",
    "            plt.title('Current Game State')\n",
    "            plt.axis('off')\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        clear_output(wait=True)\n",
    "        plt.show()\n",
    "        \n",
    "    def update_game_state(self, state):\n",
    "        self.latest_state = np.squeeze(state)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "def train_agent_with_viz():\n",
    "    env = DinoGameEnv()\n",
    "    state_size = (84, 84, 1)\n",
    "    action_size = 3\n",
    "    agent = DQNAgent(state_size, action_size)\n",
    "    gan = DinoGAN()\n",
    "    viz = TrainingVisualizer()\n",
    "    \n",
    "    batch_size = 32\n",
    "    episodes = 1000\n",
    "    \n",
    "    for episode in range(episodes):\n",
    "        state = env.reset()\n",
    "        state = np.reshape(state, [1, 84, 84, 1])\n",
    "        total_reward = 0\n",
    "        d_loss = 0\n",
    "        g_loss = 0\n",
    "        \n",
    "        for time in range(5000):\n",
    "            # Update visualization with current game state\n",
    "            viz.update_game_state(state)\n",
    "            \n",
    "            # Get action from DQN\n",
    "            action = agent.act(state)\n",
    "            \n",
    "            # Take action in environment\n",
    "            next_state, reward, done, _ = env.step(action)\n",
    "            next_state = np.reshape(next_state, [1, 84, 84, 1])\n",
    "            \n",
    "            total_reward += reward\n",
    "            \n",
    "            # Remember the transition\n",
    "            agent.remember(state, action, reward, next_state, done)\n",
    "            \n",
    "            state = next_state\n",
    "            \n",
    "            if done:\n",
    "                break\n",
    "                \n",
    "            if len(agent.memory) > batch_size:\n",
    "                agent.replay(batch_size)\n",
    "        \n",
    "        # Train GAN every 10 episodes\n",
    "        if episode % 10 == 0:\n",
    "            # Train GAN (same as before)\n",
    "            real_states = np.array(random.sample(agent.memory, batch_size))[:, 0]\n",
    "            real_states = np.squeeze(real_states)\n",
    "            \n",
    "            noise = np.random.normal(0, 1, (batch_size, gan.latent_dim))\n",
    "            gen_states = gan.generator.predict(noise)\n",
    "            \n",
    "            d_loss_real = gan.discriminator.train_on_batch(real_states, np.ones((batch_size, 1)))\n",
    "            d_loss_fake = gan.discriminator.train_on_batch(gen_states, np.zeros((batch_size, 1)))\n",
    "            d_loss = 0.5 * np.add(d_loss_real, d_loss_fake)\n",
    "            \n",
    "            noise = np.random.normal(0, 1, (batch_size, gan.latent_dim))\n",
    "            g_loss = gan.combined.train_on_batch(noise, np.ones((batch_size, 1)))\n",
    "        \n",
    "        # Update visualization\n",
    "        viz.update_metrics(total_reward, d_loss, g_loss, agent.epsilon)\n",
    "        viz.plot_metrics(episode)\n",
    "        \n",
    "        print(f\"Episode: {episode + 1}/{episodes}, Score: {total_reward}, Epsilon: {agent.epsilon:.2f}\")"
   ]
  },
